config wifi-device  radio0
        option type     mac80211
        option channel 2
        option hwmode   11g
        option path     'platform/ar934x_wmac'
        option htmode   HT20
        # REMOVE THIS LINE TO ENABLE WIFI:
        # option disabled 1

config wifi-iface
        option device   radio0
        option network  lan
        option mode     ap
        option ssid P3C_100204
        option wds      1
        option encryption psk2
        option key      12345678
